<Window x:Class="NV.DetectionPlatform.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:NV.DetectionPlatform.UCtrls"
        xmlns:ctrls="clr-namespace:NV.DRF.Controls;assembly=NV.DRF.Controls"
        Title="睿奥高清晰X光检测系统" Tag="工业无损检测软件" FontSize="15" ShowInTaskbar="True" Height="1080" Icon="NVCT.ico" Width="1500" WindowStyle="None" Style="{DynamicResource WindowStyle}" AllowsTransparency="False">
    <Window.Resources>
        <RoutedUICommand x:Key="CMDStartAcq" />
        <RoutedUICommand x:Key="CMDAutoWL" />
    </Window.Resources>
    
    <Window.CommandBindings>
        <CommandBinding Command="{StaticResource CMDStartAcq}" Executed="ProductExamStart_Click"/>
        <CommandBinding Command="{StaticResource CMDAutoWL}" Executed="_hVView_AutoWLEvent"/>
    </Window.CommandBindings>
    <Window.InputBindings>
        <KeyBinding Gesture="Ctrl+Enter" Command="{StaticResource CMDStartAcq}"/>
        <KeyBinding Gesture="Ctrl+A" Command="{StaticResource CMDAutoWL}"/>
    </Window.InputBindings>
    <Grid Background="{DynamicResource LightGrayBG}">
        <Grid.RowDefinitions>
            <RowDefinition Height="32"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <ctrls:PopubEx x:Name="pRegister" IsOpen="{Binding IsChecked,ElementName=btnRegister,Mode=TwoWay}" StaysOpen="False" AllowsTransparency="True" PopupAnimation="None" Placement="Right" PlacementTarget="{Binding ElementName=btnRegister}">
            <Border x:Name="bdrRegister" BorderBrush="Gray" Margin="1" Background="{DynamicResource LightGrayBG}">
            </Border>
        </ctrls:PopubEx>
        
        <Border Grid.Row="0" BorderThickness="0,0,0,0" BorderBrush="#FF606060">
            <Border.Effect>
                <DropShadowEffect ShadowDepth="1" Color="Gray"/>
            </Border.Effect>
            <!--菜单-->
            <Grid>
                <Menu x:Name="menuBar" Background="Transparent" Foreground="#FFF1F1F1" Margin="10,0,0,2" HorizontalAlignment="Left" VerticalAlignment="Bottom">
                    <Menu.Resources>
                        <Style TargetType="MenuItem">
                            <Setter Property="FontSize" Value="15"/>
                            <Setter Property="Margin" Value="0,0,10,0"/>
                        </Style>
                    </Menu.Resources>
                    <MenuItem Header="{DynamicResource Menu_File}">
                        <MenuItem Header="{DynamicResource Menu_File_Open}" Click="OpenFile" >
                        </MenuItem>
                        <MenuItem Header="{DynamicResource Menu_File_Close}" Click="CloseFile"/>
                        <MenuItem Header="{DynamicResource Menu_File_Save}" Click="SaveFile"/>
                        <MenuItem Header="{DynamicResource Menu_File_SaveAs}" Click="SaveAs"/>
                    </MenuItem>
                    <MenuItem Header="{DynamicResource Menu_Setting}">
                        <MenuItem Header="{DynamicResource Menu_Setting_Detector}" >
                            <MenuItem Header="{DynamicResource Menu_Setting_Detector_cfg}" Tag="DetectorSetting"  Click="SettingClick"/>
                            <!--<MenuItem Header="{DynamicResource Menu_Setting_Detector_Correction}">
                                <MenuItem Header="{DynamicResource Menu_Setting_Detector_Correction_Offset}" Tag="DetectorOffsetCorrection"  Click="SettingClick"/>
                                <MenuItem Header="{DynamicResource Menu_Setting_Detector_Correction_Gain}" Tag="DetectorGainCorrection" Click="SettingClick"/>
                                <MenuItem Header="{DynamicResource Menu_Setting_Detector_Correction_Defect}" Tag="DetectorDefectCorrection" Click="SettingClick"/>
                            </MenuItem>-->
                            <MenuItem Header="{DynamicResource Menu_Setting_Detector_Reconnection}" Click="SettingClick" Tag="DetectorReConnect"/>

                        </MenuItem>
                        <MenuItem Header="{DynamicResource Menu_Setting_HV}">
                            <MenuItem Header="{DynamicResource Menu_Setting_HV_Port}" Tag="PortSetting" Click="SettingClick" />
                            <MenuItem Header="{DynamicResource Menu_Setting_HV_PreHeat}" Tag="PreheatSetting" Click="SettingClick" />
                            <MenuItem Header="{DynamicResource Menu_Setting_HV_Reconnection}" Tag="PortReOpen" Click="SettingClick" />
                            <MenuItem Header="{DynamicResource Menu_Setting_HV_Security}" Tag="SafetySetting" Click="SettingClick"/>
                        </MenuItem>
                        <MenuItem IsCheckable="False" Header="{DynamicResource Menu_Setting_General}" Tag="GeneralSetting"  Click="SettingClick"/>
                        <MenuItem Header="{DynamicResource Menu_Setting_Label}" Tag="FilmSetting" Click="SettingClick"/>
                    </MenuItem>
                    <!--<MenuItem Header="实验(H)">
                        <MenuItem Header="静态多能采集合成" ToolTip="只能在静态单张图像模式下使用" Click="Help" Tag="MultiEnergyAvgGrab"/>
                    </MenuItem>-->
                    <MenuItem Header="{DynamicResource Menu_Help}">
                        <MenuItem Header="{DynamicResource Menu_Help_RepairReport}" Click="Help" Tag="ReportUpdate"/>
                    </MenuItem>
                    <MenuItem Header="{DynamicResource Menu_Language}">
                        <MenuItem Header="{DynamicResource Menu_Language_zhCN}" Click="LanguageSetting" Tag="CN"/>
                        <MenuItem Header="{DynamicResource Menu_Language_enUS}" Click="LanguageSetting" Tag="US"/>
                    </MenuItem>
                </Menu>

                <Viewbox HorizontalAlignment="Center" Stretch="Uniform" x:Name="vbProductInfo">
                    <TextBlock HorizontalAlignment="Center">
                        <Label Content="{DynamicResource Top_Solution}" FontSize="17"/>
                        <Label x:Name="lblExamType" Content="--" FontSize="17"/>
                        <Label x:Name="lblKV" Content="0kV" FontSize="17"/>
                        <!--<Label x:Name="lblUA" Content="0uA" FontSize="17"/>-->
                        <Label x:Name="lblTime" Content="0s" FontSize="17"/>
                        <!--<Label x:Name="lblFps" Content="0Fps" FontSize="17"/>-->
                        <Label Content="∥"/>
                <Label HorizontalAlignment="Center" FontSize="17" Content="{DynamicResource Top_ProductName}" Foreground="LightGray" />
                <TextBox IsReadOnly="True" FontSize="17" Background="Transparent" BorderThickness="0,0,0,1" Text="--" Foreground="{DynamicResource BtnForeground}" x:Name="txtProductName"/>
                    <Label HorizontalAlignment="Center" FontSize="17" Content="{DynamicResource Top_ProductType}" Foreground="LightGray" />
                <TextBox IsReadOnly="True" FontSize="17" Background="Transparent" BorderThickness="0,0,0,1" Text="--" Foreground="{DynamicResource BtnForeground}" x:Name="txtProductType"/>
                    <Label HorizontalAlignment="Center" FontSize="17" Content="{DynamicResource Top_ProductSpecification}" Foreground="LightGray" />
                <TextBox IsReadOnly="True" FontSize="17" Background="Transparent" BorderThickness="0,0,0,1" Text="--" Foreground="{DynamicResource BtnForeground}" x:Name="txtProductSpe"/>
                    <!--<Label HorizontalAlignment="Center" FontSize="17" Content="  关键词:" Foreground="LightGray" Visibility="Collapsed"/>
                <TextBox IsReadOnly="True" FontSize="17" Background="Transparent" BorderThickness="0,0,0,1" Text="" Foreground="{DynamicResource BtnForeground}" x:Name="txtProductKeywords" Visibility="Collapsed"/>-->
                    </TextBlock>
                </Viewbox>

            </Grid>
        </Border>


        <!--主界面-->
        <Grid Grid.Row="1" Margin="10,0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="130"/>
                <ColumnDefinition Width="*" x:Name="cloImageView"/>
                <ColumnDefinition Width="240"/>
            </Grid.ColumnDefinitions>

            <GridSplitter Grid.Column="0" Width="2" Background="#FF686868" IsEnabled="False">
                <GridSplitter.Effect>
                    <DropShadowEffect ShadowDepth="1" Color="Gray"/>
                </GridSplitter.Effect>
            </GridSplitter>
            <!--模块菜单-->
