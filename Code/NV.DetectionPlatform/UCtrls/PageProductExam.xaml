                        </Button.Background>
                    </Button>
                    <Button Margin="4" Content="1:1" ToolTip="{DynamicResource ExamTips_2}" Foreground="{DynamicResource BtnForeground}" Tag="Method RealSizeImage" Click="btnImgProc_Click" Style="{DynamicResource GrayButtonStyle}"/>


